<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title Shoptime
    link(href="https://use.fontawesome.com/releases/v5.0.13/css/all.css", rel="stylesheet", integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp", crossorigin="anonymous")
  body.black.font-16
    #app
      .top-bar.bg-black.gray
        .logo.font-24.bold.white Shoptime
        .tag
          .home(:class="{'active':activeMenu==='home'}", @click="activeMenu = 'home'") HOME
          .orders(:class="{'active':activeMenu==='orders'}", @click="activeMenu = 'orders'") ORDERS
          .product(:class="{'active':activeMenu==='product'}", @click="activeMenu = 'product'") PRODUCT
        .admin.white ADMIN
      .container(v-if="activeMenu==='home'")
        header
          .caption.font-24.bold OVERVIEW
          .duration
            i 2018/6/6
            i.fas.fa-caret-right.black
            i 2018/6/13
            i
            i Weekly
            i.fas.fa-caret-down.black
        .summary
          .summary-item.radius.bg-white
            .top
              i.fas.fa-hand-holding-usd.font-28
              i TOTAL REVENUE
            .bottom
              i.green.font-36 {{totalRevenue | formatCurrency}}
          .summary-item.radius.bg-white
            .top
              i.fas.fa-boxes.font-28
              i TOTAL COST
            .bottom
              i.red.font-36 {{totalCost | formatCurrency}}
          .summary-item.radius.bg-white
            .top
              i.fas.fa-money-bill.font-28
              i NET INCOME
            .bottom
              i.blue.font-36 {{(totalRevenue - totalCost) | formatCurrency}}
        .trends.radius.bg-white
          .labelString.font-24.bold ACTIVITY
          chart-component
        .statistics
          .transition-sites.radius.bg-white
            .font-24.bold Transaction Website
            .item
              img(src="data:image/svg+xml;utf8;base64,")
              .url.gray-dark facebook.com
              .income.font-20 {{45836 | formatCurrency }}
              .ratio.green
                i.fas.fa-arrow-up.pr-5.font-20
                i 20%
            .item
              img(src="data:image/svg+xml;utf8;base64,")
              .url.gray-dark youtube.com
              .income.font-20 {{23582 | formatCurrency }}
              .ratio.green
                i.fas.fa-arrow-up.pr-5.font-20
                i 12%
            .item
              img(src="data:image/svg+xml;utf8;base64,")
              .url.gray-dark ebay.com
              .income.font-20 {{2489 | formatCurrency }}
              .ratio.red
                i.fas.fa-arrow-down.pr-5.font-20
                i 15%
            .item
              img(src="data:image/svg+xml;utf8;base64,")
              .url.gray-dark wordpress.com
              .income.font-20 {{1057 | formatCurrency }}
              .ratio.red
                i.fas.fa-arrow-down.pr-5.font-20
                i 30%
          .latest-orders.radius.bg-white
            .font-24.bold Latest Orders
            .item
              img(src="http://img1.jurko.net/avatar_15063.jpg", alt="Vintage T-shirt")
              .info
                .font-20.bold Vintage T-shirt
                .middle
                  i.left
                    i.fas.fa-clock.gray-dark.font-10
                    i.gray-dark 2018/6/13 13:42
                  i.right.font-14 Total
                .bottom
                  i.left
                    i.fas.fa-male.gray-dark
                    i.gray-dark Belle Willis
                  i.right.font-20.bold {{3200 | formatCurrency}}
            .item
              img(src="http://img1.jurko.net/avatar_16227.jpg", alt="Cowboy Jacket")
              .info
                .font-20.bold Cowboy Jacket
                .middle
                  i.left
                    i.fas.fa-clock.gray-dark.font-10
                    i.gray-dark 2018/6/13 10:45
                  i.right.font-14 Total
                .bottom
                  i.left
                    i.fas.fa-male.gray-dark
                    i.gray-dark Adrian Cummings
                  i.right.font-20.bold {{2800 | formatCurrency}}
            .item
              img(src="http://img1.jurko.net/avatar_5250.gif", alt="Coach Coat")
              .info
                .font-20.bold Coach Coat
                .middle
                  i.left
                    i.fas.fa-clock.gray-dark.font-10
                    i.gray-dark 2018/6/13  8:26
                  i.right.font-14 Total
                .bottom
                  i.left
                    i.fas.fa-male.gray-dark
                    i.gray-dark Lura Holland
                  i.right.font-20.bold {{1600 | formatCurrency}}
          
        

